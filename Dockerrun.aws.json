{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "omkarsonawane/docker-multi-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "worker",
      "image": "omkarsonawane/docker-multi-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": "omkarsonawane/docker-multi-server",
      "hostname": "api_server",
      "essential": "false",
      "memory": 128
    },
    {
      "name": "nginx-proxy",
      "image": "omkarsonawane/docker-multi-nginx",
      "hostname": "nginx",
      "essential": "true",
      "memory": 128,
      "portMappings": [
        {
          "hostPort": "8080",
          "containerPort": "80"
        }
      ],
      "links": ["client", "worker", "server"]
    }
  ]
}
